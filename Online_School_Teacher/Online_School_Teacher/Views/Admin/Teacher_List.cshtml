
@{
    ViewData["Title"] = "Teacher_List";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Approve Teacher</h1>
<br />

<div class="col-12 border p-3">
    <table id="Approve" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Email</th>
                <th>Name</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

<br />

<h1>Not Approve Teacher</h1>
<br />

<div class="col-12 border p-3">
    <table id="NotApprove" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Email</th>
                <th>Name</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

<br />

<h1>Block Teachers</h1>
<br />

<div class="col-12 border p-3">
    <table id="BlockTable" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Email</th>
                <th>Name</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{

    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <script src="~/js/Admin_Approve_Teacher.js"></script>
    <script src="~/js/Admin_Non_Approve_Teacher.js"></script>
    <script src="~/js/Block_Teachers.js"></script>
}

